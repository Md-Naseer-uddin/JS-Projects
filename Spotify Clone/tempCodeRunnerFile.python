
def isprime(number):
    if number <= 1:
        return False
    for i in range(2, int(number ** 0.5) + 1):
        if number % i == 0:
            return False
    return True


def sum_of_non_primes(num):
    sum=0
    while num:
        n=num%10
        num=num//10
        prime=isprime(n)
        if prime==False:
            sum+=n
    return sum





num=int(input())
print(sum_of_non_primes(num))
print(isprime(7))